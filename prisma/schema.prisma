generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        String    @id @default(uuid())
  username  String @unique
  firstName String? 
  lastName  String? 
  password  String 
  Coloc     Coloc[]
}

model Coloc {
  id        String    @id @default(uuid())
  name      String @unique
  users     User[]
}